<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.world_tech_point.worldwide_knowledge">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Worldwide_knowledge">
        <activity android:name=".OpeningActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PrivacyPolicyActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".services.exam.ExamOptionActivity" />
        <activity android:name=".work_places.short_question.QuestionActivity" />
        <activity android:name=".work_places.ExamActivity" />
        <activity android:name=".work_places.QuizActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Worldwide_knowledge.NoActionBar">
        </activity>
    </application>

</manifest>